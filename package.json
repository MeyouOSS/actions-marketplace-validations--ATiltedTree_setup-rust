{
  "name": "setup-rust",
  "version": "1.0.8",
  "private": true,
  "description": "Setup Rust by MeyouOSS",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc -C build",
    "all": "npm run format && npm run lint && npm run build && npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ATiltedtree/setup-rust.git"
  },
  "keywords": [
    "actions",
    "rust",
    "setup"
  ],
  "author": "ATiltedTree, MeyouOSS",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^4.0.3",
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.2"
  },
  "devDependencies": {
    "@types/node": "12.12.47",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "@vercel/ncc": "^0.33.0",
    "eslint": "^7.4.0",
    "prettier": "^2.0.5",
    "typescript": "^3.9.6"
  }
}
